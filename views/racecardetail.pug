extends layout
block content
    h1= title
    - let racecarid = toShow._id
    - let driver  = toShow.driver 
    - let ranking  = toShow.ranking 
    div
      div.row
        div(class='racecarAttr mr-3') racecar ID is:
        div #{racecarid}
      div.row
        div(class='racecarAttr mr-3') racecar Type is:
        div #{driver}
      div.row
        div(class='racecarAttr mr-3') racecar Size is:
        div #{ranking}